<div class="container">
  <div class="row">
    <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
<h1>ジャンル一覧</h1>
<table>
  <tbody>
    <tr>
      <td>ジャンル</td>
    </tr>
   </tbody>
</table>
<% @genres.each do |genre| %>
  <%= genre.name%>
<% end %>
<%= form_with model: @genre,url:admin_genres_path do |f| %>
<div class="field">
  <%= f.label :ジャンル %><br>
  <%= f.text_field :name %>
</div>
<br><%= f.submit '新ジャンル', class: "btn btn-success" %>
<% end %>
<%= link_to 'ジャンル変更', admin_genres_edit_path, class: "btn btn-success"%>
</div>
  </div>
    </div>