<h1>登録情報更新</h1>
<%= form_with model: @customers,url:customers_edit_path do |f| %>
<div class="field">
  <%= f.label :姓 %><br>
  <%= f.text_field :last_name %>
</div>
<div class="field">
  <%= f.label :名前 %><br>
  <%= f.text_field :first_name %>
</div>
<div class="field">
  <%= f.label :カナ姓 %><br>
  <%= f.text_field :last_name_kana %>
</div>
<div class="field">
  <%= f.label :カナ名 %><br>
  <%= f.text_field :first_name_kana %>
</div>
<div class="field">
  <%= f.label :郵便番号 %><br>
  <%= f.text_field :postal_code %>
</div>
<div class="field">
  <%= f.label :住所 %><br>
  <%= f.text_field :address %>
</div>
<div class="field">
  <%= f.label :電話番号 %><br>
  <%= f.text_field :telephone_number %>
</div>
<div class="field">
  <%= f.label :メールアドレス %><br />
  <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
</div>
<div class="field">
  <%= f.label :パスワード %><br />
  <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
</div>
<%= f.submit '登録情報変更'%>
<% end %>